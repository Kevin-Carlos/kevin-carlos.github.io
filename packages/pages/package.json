{
  "name": "kevin-carlos.github.io",
  "homepage": "https://kevin-carlos.github.io",
  "version": "2.0.0",
  "scripts": {
    "dev": "vite",
    "prebuild": "tsc",
    "build": "yarn prebuild && vite build",
    "serve": "yarn prebuild && vite preview",
    "format": "prettier --write",
    "format:all": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "lint": "eslint --fix",
    "lint:all": "eslint --fix \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "deploy": "gatsby build && gh-pages -d public -b master",
    "updateSDKs": "yarn dlx @yarnpkg/sdks vscode",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "dependencies": {
    "grommet": "^2.18.0",
    "history": "^5.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.17.0",
    "react-router": "6.0.0-beta.5",
    "react-router-dom": "6.0.0-beta.5",
    "react-spring": "^9.2.4",
    "react-tsparticles": "^1.35.2",
    "recoil": "^0.4.1",
    "styled-components": "^5.3.1"
  },
  "devDependencies": {
    "@types/eslint": "^7.28.0",
    "@types/react": "^17.0.26",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.14",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "@vitejs/plugin-react": "^1.0.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.0",
    "lint-staged": "^11.1.2",
    "pinst": "^2.0.0",
    "polished": "^4.1.3",
    "prettier": "^2.4.1",
    "react-is": "^17.0.2",
    "typescript": "^4.4.3",
    "vite": "^2.6.2"
  }
}
