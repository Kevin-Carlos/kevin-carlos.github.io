    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "polished": "4.3.1",
    "postcss": "^8.4.38",
    "tailwind-merge": "^2.5.2",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.1.6",
