    "@remix-run/react": "2.11.2",
    "isbot": "^4.1.0",
    "phosphor-react": "1.4.0",
    "react": "18.3.1",
    "react-aria-components": "^1.3.1",
    "react-dom": "18.3.1",
    "react-globe.gl": "2.27.2",
    "react-spring": "9.7.4",
    "remix-utils": "7.6.0",
    "three": "0.168.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20240903.0",
